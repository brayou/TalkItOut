<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#f7993b" />
            <gradient android:angle="-90" android:startColor="#f7993b" android:endColor="#cc7a00" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#f7993b" />
            <solid android:color="#f7993b"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#f7993b" />
            <gradient android:angle="-90" android:startColor="#f7993b" android:endColor="#f7993b" />
        </shape>
    </item>
</selector>